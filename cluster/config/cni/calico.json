{
    "name": "calico-k8s-network",
    "cniVersion": "0.1.0",
    "type": "calico",
    "etcd_endpoints": "${etcd_urls}",
    "log_level": "info",
    "ipam": {
        "type": "host-local",
        "subnet": "usePodCidr"
    },
    "kubernetes": {
        "kubeconfig": "/etc/kubernetes/secrets/calico.kubeconfig"
    }
}
